{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["ES6"],
    "module": "commonjs",
    "baseUrl": ".",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./*"],
  "exclude": ["node_modules"]
}